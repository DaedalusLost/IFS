h4.uk-padding-remove.uk-margin-remove {{ sectionName }}
    ul(ng-if="accuracy").uk-list.uk-list-striped
        li 
            p.uk-display-inline Total Words : {{ (accuracy[0].value + accuracy[1].value) || 0 }}
        li 
            p.uk-display-inline Correct Words : {{accuracy[0].value || 0}}
        li
            p.uk-display-inline Misspelt Words : {{accuracy[1].value || 0}}
